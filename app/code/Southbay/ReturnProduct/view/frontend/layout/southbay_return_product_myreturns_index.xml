<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Mis Devoluciones</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Southbay\ReturnProduct\Block\Frontend\MyReturns"
                   name="southbay_return_product_history_content"
                   template="Southbay_ReturnProduct::my_returns.phtml"
                   cacheable="false"/>
        </referenceContainer>
    </body>
</page>
