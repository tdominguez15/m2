<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/southbay/sap/docs" method="POST">
        <service class="Southbay\ReturnProduct\Api\SapDocApiInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/southbay/sap/invoice" method="POST">
        <service class="Southbay\ReturnProduct\Api\SapInvoiceApiInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/southbay/sap/order/confirmation" method="POST">
        <service class="Southbay\ReturnProduct\Api\SapOrderConfirmationApiInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
